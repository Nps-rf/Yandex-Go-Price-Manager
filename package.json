{
  "name": "igo",
  "productName": "yandex-go-price-manager",
  "version": "1.2.1",
  "description": "My Electron application description",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron-forge start",
    "prestart": "webpack --config webpack.renderer.config.js --mode=development --progress",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "prebuild": "webpack --config webpack.main.config.js --mode=development --progress",
    "build": "electron-builder",
    "publish": "electron-forge publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "build": {
    "appId": "com.freedom.ygpm",
    "productName": "YangexGoPriceManager",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "build/"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/plugin-fuses": "^7.2.0",
    "@electron/fuses": "^1.7.0",
    "css-loader": "^6.10.0",
    "electron": "^29.0.1",
    "electron-builder": "^24.12.0",
    "eslint": "^8.48.0",
    "mini-css-extract-plugin": "^2.8.1",
    "path-browserify": "^1.0.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-obfuscator": "^3.5.1"
  },
  "keywords": ["yandex", "yangex-go"],
  "author": {
    "name": "Николай Пикалов",
    "email": "divine.nikolai@gmail.com"
  },
  "license": "MIT"
}
